.clear {
    zoom: 1;
}
.clear:after {
    content: '';
    display: block;
    clear: both;
}
.h-module-5-27
    .h-banner
        height 750px
        background url('') center center no-repeat
        &.h468
            height 468px
    .h-main-service
        padding-top 100px
    .h-main-title
        padding 60px 0
        font-size 48px
        color #000000
        font-style italic
        font-weight bold
