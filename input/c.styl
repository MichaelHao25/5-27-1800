        .h-container
            position relative
            z-index 1
            height 100%
            display flex
            align-items center
            justify-content center
            @media(max-width 750px)
                padding 50px 0
                display block
        .layout-pic
            position absolute
            left 0
            top 0
            bottom 0
            width 50%
            background url('../img/aacb22d3a2465e5cd2741b25234c2012-985+765.jpg') right center/auto 100% no-repeat            
