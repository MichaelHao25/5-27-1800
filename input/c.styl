                width 100%
                background-size cover
        .left-pic
        .right-content
            width 564px
            @media(max-width 750px)
                width 100%
        .left-pic
            text-align right
            @media(max-width 750px)
                text-align center
            img 
                max-width 334px
                width 100%
                vertical-align middle
        .right-content
            box-sizing border-box
            padding 30px
            height 350px
            @media(max-width 750px)
                padding 15px
                height auto 
                background  rgba(#fff,0.8)
            h3
                padding 20px 0 10px
                font-size 22px
                color #000000
            p
                font-size 14px
                color #888888
                line-height 30px
            a
                margin 20px 0 0 20px
                display block
                width 100px
                height 34px
                font-size 14px
                color #8de09d
                text-align center
                line-height 34px
                border 1px solid #8de09d
                transition 0.3s
                &:hover
                    background #8de09d
                    color #ffffff
    .h-main-our-news
        .wid
            display flex
            justify-content space-between
            @media(max-width 750px)
                flex-direction column
                max-width 500px
                margin 0 auto
        .item
            width 274px
            @media(max-width 750px)
                flex 1
                width 100%
                &+.item
                    margin-top 30px
            &:hover
                a
                    background #4cca9c
                .pic
                    .layout
                        opacity 1
            .pic
                position relative
                img
                    display block
                    width 100%
                .layout
                    position absolute
                    bottom 0
                    left 0
                    right 0
                    top 0
                    background rgba(#4cca9c,0.8)
