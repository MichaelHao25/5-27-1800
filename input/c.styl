            .h-row
                position relative
            h3
                padding 25px 0 20px
                font-size 18px
                color #2b2b2b
                line-height 30px
            .date
                font-size 14px
                color #7f7f7f
                position absolute
                right 0
                bottom 20px
                line-height 30px
                padding-left 20px
                background  url('../img/6b9ef9ba7cea4eed21b3eb32ebb4c320-15+15.png') left center no-repeat
            p
                font-size 14px
                color #7f7f7f
                line-height 30px
            a
                margin-top 10px
                display block
                width 104px
                height 30px
                font-size 12px
                color #ffffff
                text-align center
                line-height 30px
                font-style italic
                background #8a8a8a
                transition 0.3s
                border-radius 5px
    .h-main-join-us
        padding-bottom 100px
        .wid
            display flex
            justify-content space-between
            @media(max-width 750px)
                flex-direction column
                align-items center
        .item
            text-align center
            display flex
            flex-direction column
            @media(max-width 750px)
                flex 1
                max-width 500px
                padding 0 15px
            &:hover
                .pic
                    .layout
                        transform scale(1)
            &:nth-child(even)
                .content
                    order -1
                    @media(max-width 750px)
                        order 0
                .pic
                    .layout
                        top 20px
                        bottom -20px
                        @media(max-width 750px)
                            top -20px
                            bottom 20px
            .pic
                position relative
                .layout
                    position absolute
                    top -20px
                    left -20px
                    right -20px
                    bottom 20px
                    border 1px solid #dadada
                    background url('../img/d565f9f6b7b78b17aa88be353437bfaf-54+54.png') center center no-repeat
                    transform scale(0)
                    transition 0.3s
                img
                    display block
                    width 100%
                    
            h3
                padding 10px 0 0
                font-size 18px
                color #4c4c4c
                line-height 2
            .en
                padding-bottom 10px
                font-size 14px
                color #888888
                line-height 2
            p
                position relative
                padding 20px 0
                font-size 12px
                color #888888
